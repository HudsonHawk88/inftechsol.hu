    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "scripts": {
    "install-root": "npm i",
    "install-frontend": "cd src/frontend && npm i",
    "install-server": "cd src/server && npm i",
    "install-all": "npm run install-root && npm run install-frontend && npm run install-server",
    "build": "webpack -p",
    "deploy": "npm run build && git commit -a -m deployment && git push heroku master",
    "dev": "webpack-dev-server --open",
    "server": "cross-env NODE_ENV=production babel-node src/server/index.js",
    "server-dev": "nodemon -r dotenv/config src/server/index.js",
    "start-dev": "concurrently \"npm run server-dev\" \"npm run dev\"",
    "server-spec": "babel-node src/server/server.spec",
    "start": "npm run server"
  },
  "keywords": [
    "react",
    "express"
